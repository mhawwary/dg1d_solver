[Case]
 num_elements = 10
 x_begin = 0.0
 x_end = 1.0
 uniform_grid = 1
 refinement_level = 0

[Simulation] 
 print_freq = 1000

[wave]
 wave_speed = 1;

[space_solver]
  polynomial_order = 2;
  upwind_param=1; # 1: fully upwind, 0: fully central, (0,1) hybrid
 
[time_solver]
 calculate_dt_flag=1 # 1: specify CFL and calc dt, 0: specify dt and calc CFL
 CFL_no = 0.2

 dt = 1e-5
 initial_time = 0

 final_time = 2e4
 
 maximum_iteration = 1e6

 [./explicit]

  RK_order = 3
